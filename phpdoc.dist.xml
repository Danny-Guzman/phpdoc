<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.phpdoc.org" xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd">
    <paths>
        <output>.docs</output>
    </paths>
    <title>CAWeb</title>
    <version number="3.1.0">
        <api>
            <default-package-name>CAWeb</default-package-name>
            <source dsn=".">
            </source>
            <ignore>
                <path>vendor/**/*</path>
                <path>node_modules/**/*</path>
            </ignore>
        </api>
    </version>
          
    <setting name="template_version" value="6.2.1" />
    <setting name="colorscheme" value="oceanside" />

    <!-- Alerts MUST BE:
        1) An array [] 
        2) Individual alerts are objects {} in the array
        3) Individual alerts are separated by a comma with no spaces 
        4) Individual alert properties are separated by a comma with no spaces -->
    <setting name="alerts" value=" [{'heading' : 'Important','icon': 'info','msg' : 'This documentation was generated using phpDocumentor and the CA State phpDocumentor Template','link':'#','button':'Read More' }]" />


</phpdocumentor>